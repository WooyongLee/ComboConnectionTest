<Application x:Class="ComboBrokerTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ComboBrokerTest"
             xmlns:viewModelConverter="clr-namespace:ComboBrokerTest.ViewModelConverter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <viewModelConverter:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter"></viewModelConverter:InverseBooleanToVisibilityConverter>
    </Application.Resources>
</Application>
